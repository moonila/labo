<configuration>
		
	<appender name="console" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{dd-MM-yy;HH:mms}: RecopieProfil: %c{0}: %-4p: %m %n</pattern>
		</encoder>
	</appender>
	
	<logger name="org.moonila.planning.manager" level="debug">

    </logger>
	<root level="error">
		<appender-ref ref="console" />
	</root>
</configuration> 